.goals-noti h3{@include text($size:$text02,$weight:bold);margin-top:13px;}
.goals-noti h3 + ul{margin-top:20px;}

.goals-use-best{margin-top:34px;margin-bottom:196px;}

.ranking-list .ranking-tit{font-size:18px;line-height:22px;}
.ranking-list .ranking-tit strong{font-weight:bold;}
.ranking-list .ranking-tit + .ranking-list{margin-top:27px;}
.ranking-list ul.ranking-list li{padding-left:33px;position:relative;}
.ranking-list ul.ranking-list li:before{content:'';@include text($size:$text03,$weight:bold);line-height:24px;width:24px;height:24px;position:absolute;left:0;top:-2px;text-align:center;background:$kb-ui07;display:block;}
.ranking-list ul.ranking-list li.rank:before{background:$color-yellow;}
.ranking-list ul.ranking-list li:nth-child(1):before{content:'1';}
.ranking-list ul.ranking-list li:nth-child(2):before{content:'2';}
.ranking-list ul.ranking-list li:nth-child(3):before{content:'3';}
.ranking-list ul.ranking-list li:nth-child(4):before{content:'4';}
.ranking-list ul.ranking-list li:nth-child(5):before{content:'5';}
.ranking-list ul.ranking-list li + li{margin-top:19px;}

.goals-setting{padding-top:24px;position:fixed;bottom:52px;width:100%;z-index:1;}
.goals-setting h2{@include text($text01,$weight:bold)}
.goals-setting h2 + p{@include text($text02);margin-top:10px;}
.goals-setting h2 + p strong{@include text($weight:bold)}
