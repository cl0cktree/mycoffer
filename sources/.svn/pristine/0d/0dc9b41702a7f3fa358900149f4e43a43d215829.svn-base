.character-graph[data-theme=ago]{padding-top:80px;}
.character-graph[data-theme=ago] .graph-area > span:after{background:url(../images/goal/progress_ago.png) no-repeat;background-size:contain;width:76px;height:58px;}
.character-graph[data-theme=bb]{padding-top:80px;}
.character-graph[data-theme=bb] .graph-area > span:after{background:url(../images/goal/progress_bb.png) no-repeat;background-size:contain;width:61px;height:61px;}
.character-graph[data-theme=lamu]{padding-top:80px;}
.character-graph[data-theme=lamu] .graph-area > span{background-color:$color-yellow;}
.character-graph[data-theme=lamu] .graph-area > span:after{background:url(../images/goal/progress_lamu.png) no-repeat;background-size:contain;width:41px;height:78px;}
.character-graph[data-theme=kolly]{padding-top:80px;}
.character-graph[data-theme=kolly] .graph-area > span{background-color:#c6d65e;}
.character-graph[data-theme=kolly] .graph-area > span:after{background:url(../images/goal/progress_kolly.png) no-repeat;background-size:contain;width:62px;height:73px;}
.character-graph[data-theme=diver]{padding-top:80px;}
.character-graph[data-theme=diver] .graph-area > span{background:$color-yellow;}
.character-graph[data-theme=diver] .graph-area > span:after{background:url(../images/goal/progress_diver.png) no-repeat;background-size:contain;width:76px;height:66px;right:0;bottom:0;}

.character-graph.goal{padding-top:80px;}
.character-graph.goal[data-theme=bb] .graph-area > span:after{background:url(../images/goal/progress_bb2.png) no-repeat;background-size:contain;width:61px;height:61px;right:0;}
.character-graph.goal[data-theme=kiki] .graph-area > span:after{background:url(../images/goal/progress_kiki3.png) no-repeat;background-size:contain;width:46px;height:73px;right:10px;}
.character-graph.goal[data-theme=ago] .graph-area > span:after{background:url(../images/goal/progress_ago2.png) no-repeat;background-size:contain;width:43px;height:82px;right:10px;}
.character-graph.goal[data-theme=lamu] .graph-area > span:after{background:url(../images/goal/progress_lamu2.png) no-repeat;background-size:contain;width:50px;height:80px;right:10px;}
.character-graph.goal[data-theme=kolly] .graph-area > span:after{background:url(../images/goal/progress_kolly2.png) no-repeat;background-size:contain;width:57px;height:67px;right:10px;}
.character-graph.goal .graph-area .flag{position:absolute;right:-10px;bottom:10px;}

.character-graph.goal.success{padding-top:60px;}
.character-graph.goal.success[data-theme=bb] .graph-area > span:after{background:url(../images/goal/progress_bb3.png) no-repeat;background-size:contain;width:46px;height:42px;right:10px;bottom:10px;}

.character-graph.goal.fail{margin-top:30px;padding-top:0;}
.character-graph.goal.fail .ico-area{position:absolute;top:0;left:calc(50% - 39px);}
.character-graph.goal.fail .ico-area img{width:77px;}

.character-graph[data-theme=coffee]{padding-top:50px;}
.character-graph[data-theme=coffee] .graph-area{background:none;}
.character-graph[data-theme=coffee] .graph-area > span{height:24px;border-radius:0 12px 12px 0;}
.character-graph[data-theme=coffee] .graph-area > span{background-color:$color-yellow;}
.character-graph[data-theme=coffee] .graph-area.sub > span{background-color:#e0e4e6;}
.character-graph[data-theme=coffee] .graph-area > span:after{background:url(../images/goal/progress_coffee.png) no-repeat;background-size:contain;width:33px;height:42px;right:-15px;}
.character-graph[data-theme=coffee] .graph-area.sub > span:after{background:none;width:1px;height:1px;}
.character-graph[data-theme=coffee] .graph-txt + .graph-area.sub{margin-top:16px}