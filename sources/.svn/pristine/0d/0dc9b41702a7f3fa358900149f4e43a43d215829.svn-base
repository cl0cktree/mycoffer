.goals-others .chart-area.dbl-bar .txt-gray{@include text($size:$text03);padding-left:16px;position:relative;}
.goals-others .chart-area.dbl-bar .txt-gray:before{content:'';width:9px;height:9px;position:absolute;display:block;border-radius:50%;top:5px;left:0;background:$kb-ui07;}
.goals-others .chart-area.dbl-bar .txt-yellow{@include text($size:$text03);padding-left:16px;position:relative;}
.goals-others .chart-area.dbl-bar .txt-yellow:before{content:'';width:9px;height:9px;position:absolute;display:block;border-radius:50%;top:5px;left:0;background:$color-yellow;}
