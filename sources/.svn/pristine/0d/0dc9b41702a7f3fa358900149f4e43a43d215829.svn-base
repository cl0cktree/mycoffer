.goals-setting .character-graph .graph-txt p:after{content:attr(data-unit);}


.text-trophy{padding-left:65px;position:relative;@include text($text02);}
.text-trophy .ico{display:block;position:absolute;width:50px;height:50px;border:1px solid $kb-ui07;border-radius:50%;left:0;top:0;background:#fff;text-align:center;}
.text-trophy .ico i{margin-top:10px;}
.text-trophy p{@include text($weight:bold)}
.text-trophy p + span{margin-top:13px;}
.text-trophy span{display:block;}
.text-trophy strong{font-weight:bold;}

.goals-no-spand{padding-top:24px;position:fixed;bottom:52px;width:100%;z-index:1;}
