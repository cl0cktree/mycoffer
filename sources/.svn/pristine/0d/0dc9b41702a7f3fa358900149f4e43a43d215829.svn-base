.character-graph .graph-txt{font-size:14px}
.character-graph .graph-txt .start-txt{margin-left:0}
.character-graph .graph-txt .end-txt{margin-right:0}
.character-graph .graph-area > span:after{color:transparent;}

.character-graph[data-theme=kiki]{padding-top:80px;}
