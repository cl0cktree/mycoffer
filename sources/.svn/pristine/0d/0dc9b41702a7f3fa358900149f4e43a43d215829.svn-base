.goals-char .char-box .swiper-wrapper .swiper-slide > button.is-selected:after{content:'';position:absolute;width:100%;height:60px;background:url(../images/goal/complete_icon.png) center center no-repeat;background-size:contain;top:52px;right:0;z-index:2;}
.goals-char .char-box .swiper-wrapper .swiper-slide > button.is-selected:before{content:'선택완료';background:rgba(0,0,0,.6);position:absolute;top:0;display:block;width:100%;height:100%;font-size:24px;font-weight:bold;color:#fff;line-height:300px;z-index:1;}
.goals-char .char-box .swiper-wrapper button p.name{position:absolute;width:100%;@include text($size:$text02,$weight:bold);top:25px;text-align:center;}
.goals-char .char-box .swiper-wrapper button p.txt{position:absolute;width:100%;@include text($size:$text03);bottom:25px;text-align:center;}
.goals-char .char-box .swiper-wrapper button p.txt strong{font-weight:bold;}
.goals-char .img-unit{margin-top:20px;text-align:center;}
.goals-char .img-unit.all-char > img{max-width:296px;}
